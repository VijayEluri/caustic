
{
	"load" : "http://webapps.nyc.gov:8084/CICS/fin1/find001I",
	"method" : "post",
	"posts" : {
		"FHOUSENUM" : "{{Number}}",
		"FSTNAME"   : "{{Street}}",
		"FBORO"     : "{{Borough}}",
		"FAPTNUM"   : "{{Apt}}"
	},
	"then" : {
			"name" : "Owner of {{Apt}} {{Number}} {{Street}}, Borough {{Borough}}",
			"find" : "<input\\s+type=\"hidden\"\\s+name=\"ownerName\\d?\"\\s+value=\"\\s*(\\w[^\"]*?)\\s*\"",
			"replace" : "$1"
		}
}