
{
	"url" : "http://www.google.com/search?q={{query}}",
	"finds_many"  : {
		"name"   : "after",
		"pattern"     : "{{query}}\\s+(\\w+)",
		"replacement" : "$1",
		"then" : {
			"url" : "http://www.google.com/search?q={{after}}",
			"finds_many" : {
				"name"   : "what do you say after '{{after}}'?",
				"pattern"     : "{{after}}\\s+(\\w+)",
				"replacement" : "I say '$1'!"
			}
		}
	}
}
