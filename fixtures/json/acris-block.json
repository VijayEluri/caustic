
{
	"url" : "http://a836-acris.nyc.gov/Scripts/DocSearch.dll/BBLResult",
	"name" : "ACRIS BBLResult",
	"cookies" : {
		"JUMPPAGE" : "YES"
	},
	"posts" : {
		"hid_borough" : "{{Borough}}",
		"hid_borough_name" : "",
		"hid_block" : "{{Block}}",
		"hid_lot"   : "0000",
		"hid_selectdate": "DR",
		"hid_datefromm" : "{{FromMonth}}",
		"hid_datefromd" : "{{FromDay}}",
		"hid_datefromy" : "{{FromYear}}",
		"hid_datetom" : "{{ToMonth}}",
		"hid_datetod" : "{{ToDay}}",
		"hid_datetoy" : "{{ToYear}}",
		"hid_doctype" : "{{DocType}}",
		"hid_doctype_name" : "",
		"hid_max_rows" : "10000",
		"hid_page" : "",
		"hid_ReqId" : "",
		"hid_SearchType" : "BBL",
		"hid_ISIntranet" : "N",
		"hid_EmployeeID" : ""
	},
	"find" : {
		"name" : "table",
		"save" : false,
		"pattern" : "<!--Table Begin!-->(.*?)<!--Table End-->",
		"match" : 0,
		"find" : {
			"extends" : { "$ref" : "shared/html-row.json" },
			"name"      : "record",
			"save"      : false,
			"min"  : 2,
			"max"  : -1,
			"find" : [{
				"name"        : "Borough",
				"pattern"     : ".",
				"replacement" : "{{Borough}}",
                                "match"       : 0
			},{
				"name"        : "Block",
				"pattern"     : ".",
				"replacement" : "{{Block}}",
                                "match"       : 0
			},{
				"name"        : "Doc ID",
				"pattern"     : "go_detail\\('([^']+)'\\)",
				"replacement" : "$1",
                                "match"       : 0
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Reel/Pg/File",
				"match"   : 1
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "CRFN",
				"match"   : 2
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Lot",
				"match"   : 3
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Partial",
				"match"   : 4
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Recorded / Filed",
				"match"   : 5
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Document Type",
				"match"   : 6
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Pages",
				"match"   : 7
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Party1",
				"match"   : 8
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Party2",
				"match"   : 9
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Party 3/ Other",
				"match"   : 10
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "More Party 1/2 Names",
				"match"   : 11
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Corrected/ Remarks",
				"match"   : 12
			},{
				"extends" : { "$ref" : "shared/acris-column.json" },
				"name"    : "Doc Amount",
				"match"   : 13
			}]
		}
	}
}