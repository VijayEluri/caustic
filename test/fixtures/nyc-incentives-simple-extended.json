
{
	"url" : "https://a836-propertyportal.nyc.gov/Default.aspx",
	"then": [{
		"url" : "https://a836-propertyportal.nyc.gov/searchdetails.aspx",
		"cookies" : {
			"style"   : "default",
			"borough" : "{{Borough Number}}",
			"block"   : "{{Block}}",
			"lot"     : "{{Lot}}"
		},
		"finds_one" : [
		{
			"name"    : "Borough Number",
			"pattern" : ".",
			"replacement" : "{{Borough Number}}"
		},{
			"name"    : "Block",
			"pattern" : ".",
			"replacement" : "{{Block}}"
		},{
			"name"    : "Lot",
			"pattern" : ".",
			"replacement" : "{{Lot}}"
		},{
			"name"    : "Owner Name(s)",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblOwnerNames\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Address",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblStreetAddress\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Zip Code",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblZip\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Borough",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblBorough\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Year Built",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblYearBuilt\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Number of Buildings",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblNumberOfBuildings\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Number of Stories",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblNumberOfStories\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Number of Units",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblNumberOfUnits\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Building Dimensions Front",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblBuildingFront\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Building Dimensions Depth",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblBuildingDepth\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Building Dimensions Size",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblBuildingSize\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Lot Size Front",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblLotFront\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Lot Size Depth",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblLotDepth\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Lot Size Size",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblLotSize\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Corner",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblCorner\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Building Area",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblBuildingArea\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Current Tax Year",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblCurrentTaxYear\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Building Class",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblBuildingClassDesc\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Tax Class",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblTaxClassCd\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Tax Rate",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblTaxRate\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Market Value",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblMarketValue\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Assessed Value",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblAssessedValue\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Land Assessed Value",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl02_lblLandAssessedValue\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Benefit Name",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblName\">([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Benefit Amount",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBenefitAmt\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Current Benefit Year",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblExemptYear\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Number of Benefit Years",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblYearsOfExempt\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Benefit Type",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBenefitType\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Benefit Start Date",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBenefitStartDt\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Benefit End Date",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBenefitEndDt\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Ineligible Commercial %",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblExcessCommPct\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Ineligible Residential %",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblExcessResPct\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Government Financed",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblGovtFinanced\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Project Start Date",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblProjectStartDt\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Base Year",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBaseYear\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Base Year Assessed Value",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBaseYrAV\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		},{
			"name"    : "Benefit for Tax Year",
			"pattern" : "<span\\s+id=\"ctl00_SampleContent_ctl04_lblBenTaxYear\"[^>]*>([^<]+)</span>",
			"replacement" : "$1"
		}]
	}]
}